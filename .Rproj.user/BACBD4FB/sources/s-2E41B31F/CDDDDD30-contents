#load in the libraries needed
library(shiny)

#Define the appearance of the webpage
ui <- fluidPage(
  sidebarLayout(
    titlePanel('New Zealand Income Inequality'),
    sidebarPanel(
      titlePanel("Filters"),
      
    )
  ),
  mainPanel(
    plotOutput("Condidence_plot")
  )
  
)

#Function to manage user input and output
server <- function(input, output) {}

#link both functions
shinyApp(ui = ui, server = server)